    <div class="main">
        <div class="header-main">
            <h2>ข่าวสาร</h2>
        </div>
        <div class="news">
            <section class="articles">
                <% data.forEach(news => { %>
                    
                 <article>
                    <div class="article-wrapper">
                        <figure>
                            <img src="/img/cp2.png" alt="" />
                        </figure>
                        <div class="article-body">
                            <h2><%= news.title %></h2>
                            <p class="update">update <%= news.updatedAt.toDateString() %></p>
                            <p>
                                <%= news.body %>
                            </p>
                            <a href="/news/<%= news._id %>" class="read-more">
