<main>
    <div class="main">

        <div class="header-main">
            <h2>ข่าวสารและกำหนดการ </h2>
        </div>
        <div class="content-sec">
            <div class="title">
                <h2>
                    <%= news.title %>
                </h2>
                <p>
                    <%= news.updatedAt.toLocaleString('th-TH', { year: 'numeric' , month: 'numeric' , day: 'numeric' });
                        %>
                </p>
            </div>
            <div class="body-details" contenteditable="fasle">
                <%- test %>
            </div>
        </div>
        <div class="request-doc" id="request-doc">
            <div class="xbar">
                <h3>เอกสาร</h3>
                <span class="material-icons-sharp expand_more" id="expand_more">expand_more</span>
            </div>
            <embed class="" src="/uploads/2024-01-29T06-21-12.289Z-DIP_LAB2.pdf" type="application/pdf" width="100%"
                height="100%" />
        </div>


    </div>
