                                                                                                    <option
                                                                                                        value="<%= index %>">
                                                                                                        ไม่ผ่าน</option>
                                                                                                    <% } %>
                                                                                                        <% } %>
                                                                </select>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>
