                                                        <div class="status-approve pendding">
                                                        <a>รอหนังสือตอบรับ
                                                    <% } else if (element.accepted_company_status == '1') { %>
                                                            <% if (element.sended_company_status == '0') { %>
                                                                <div class="status-approve pendding">
                                                                <a>รอหนังสือส่งตัว
                                                            <% } else if (element.sended_company_status == '1') { %>

                                                                <% if (element.certificate_info.status == '0') { %>
                                                                    <div class="status-approve cp">
                                                                    <a>กำลังฝึกงาน
                                                                    <% } else if (element.certificate_info.status == '1') { %>
                                                                    <div class="status-approve success">
